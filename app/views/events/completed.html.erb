<div class="container">
<h1>List of Completed Events</h1>
  <div class = "row">
    <div class="col-md-8 col-md-offset-2 events-list">
      <ul class="list-group">
        <% @events.completed.each do |event| %>
          <li>
            <%= event.name %>
          </li>
        <% end %>
      </ul>
    </div> <!-- col -->
  </div> <!-- row -->
</div>
